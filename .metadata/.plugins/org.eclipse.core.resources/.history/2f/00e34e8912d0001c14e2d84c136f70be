package com.lms.accesslibrary.dto;

import org.springframework.stereotype.Component;

import lombok.Data;

@Data
@Component
public class CheckoutResponse {
	private String checkoutTtrackingNumber;
	private String message;

	public String getCheckoutTtrackingNumber() {
		return checkoutTtrackingNumber;
	}

	public void setCheckoutTtrackingNumber(String checkoutTtrackingNumber) {
		this.checkoutTtrackingNumber = checkoutTtrackingNumber;
	}

	public String getMessage() {
		return message;
	}

	public void setMessage(String message) {
		this.message = message;
	}
	
}
